{
  "packageRules": [
    {
      "matchDatasources": ["crate"],
      "matchDepNames": [
        "chrono",
        "log",
        "serde",
        "serde_json",
        "xdg"
      ],
      "matchUpdateTypes": ["minor", "patch", "pin", "digest"],
      "automerge": true
    },
    {
      "matchDatasources": ["crate"],
      "matchDepNames": [
        "config",
        "fern",
        "ratatui",
        "reqwest"
      ],
      "matchUpdateTypes": ["patch", "pin", "digest"],
      "automerge": true
    },
    {
      "matchDatasources": ["docker"],
      "matchPackagePatterns": [
        "^google/cloud-sdk",
        "^thekevjames/gcloud-pubsub-emulator$",
        "^quay.io/thekevjames/gcloud-pubsub-emulator$"
      ],
      "matchUpdateTypes": ["major", "minor", "patch", "pin", "digest"],
      "automerge": true
    },
    {
      "matchDatasources": ["docker"],
      "matchPackagePatterns": [
        "^thekevjames/",
        "^quay.io/thekevjames/"
      ],
      "matchUpdateTypes": ["minor", "patch", "pin", "digest"],
      "automerge": true
    },
    {
      "matchDatasources": ["docker"],
      "matchDepNames": [
        "alpine/git",
        "bitnami/kube-state-metrics",
        "busybox",
        "caddy",
        "cimg/gcp",
        "curlimages/curl",
        "docker",
        "ghcr.io/gethomepage/homepage",
        "hashicorp/terraform",
        "moss/xelatex",
        "node",
        "nodejs/node",
        "pandoc/core",
        "surnet/alpine-wkhtmltopdf",
        "quay.io/linuxserver.io/jellyfin",
        "quay.io/linuxserver.io/prowlarr",
        "quay.io/linuxserver.io/radarr",
        "quay.io/linuxserver.io/sonarr",
        "quay.io/linuxserver.io/syncthing",
        "quay.io/prometheus/prometheus"
      ],
      "matchUpdateTypes": ["minor", "patch", "pin", "digest"],
      "automerge": true
    },
    {
      "matchDatasources": ["docker"],
      "matchDepNames": [
        "alpine",
        "python",
        "quay.io/prometheus/pushgateway"
      ],
      "matchUpdateTypes": ["patch", "pin", "digest"],
      "automerge": true
    },
    {
      "matchDepTypes": ["action"],
      "matchUpdateTypes": ["minor", "patch", "pin", "digest"],
      "pinDigests": true,
      "automerge": true
    },
    {
      "matchDatasources": ["pypi"],
      "matchPackagePatterns": [
        "^coveralls$",
        "^gcloud-aio",
        "^gcloud-rest"
      ],
      "matchUpdateTypes": ["minor", "patch", "pin", "digest"],
      "automerge": true
    },
    {
      "matchDatasources": ["pypi"],
      "matchDepNames": [
        "aiohttp",
        "fava",
        "flake8-2020",
        "flake8-broken-line",
        "flake8-builtins",
        "flake8-comprehensions",
        "google-api-python-client",
        "google-auth",
        "google-cloud-aiplatform",
        "google-cloud-bigquery",
        "google-cloud-pubsub",
        "google-cloud-speech",
        "google-cloud-storage",
        "hiredis",
        "jinja2",
        "jira",
        "jsonpath-ng",
        "hypothesis",
        "openai",
        "prometheus-fastapi-instrumentator",
        "poetry",
        "pydantic",
        "pydantic-settings",
        "pygments",
        "pyjson5",
        "pytest",
        "pytest-asyncio",
        "pytest-cases",
        "pytest-cov",
        "pytest-datadir",
        "pytest-mock",
        "redis",
        "requests",
        "sh",
        "sphinx",
        "sqlalchemy",
        "tqdm"
      ],
      "matchUpdateTypes": ["minor", "patch", "pin", "digest"],
      "automerge": true
    },
    {
      "matchDatasources": ["pypi"],
      "matchDepNames": [
        "fastapi",
        "fastapi-slim",
        "flask",
        "google-cloud-pubsub",
        "httptools",
        "httpx",
        "pandas",
        "pip",
        "plotly",
        "prometheus-client",
        "transformers",
        "typer",
        "typer-slim",
        "uvicorn"
      ],
      "matchUpdateTypes": ["patch", "pin", "digest"],
      "automerge": true
    },
    {
      "matchDatasources": ["terraform-module", "terraform-provider"],
      "matchDepNames": [
        "cloudflare",
        "confluent",
        "confluentinc/confluent",
        "github",
        "google",
        "grafana",
        "hashicorp/cloudflare",
        "hashicorp/google",
        "hashicorp/grafana",
        "hashicorp/kubernetes",
        "integrations/github",
        "kubernetes"
      ],
      "matchUpdateTypes": ["minor", "patch", "pin", "digest"],
      "automerge": true
    },
    {
      "matchDatasources": ["github-releases"],
      "matchDepNames": [
        "greenpau/caddy-security"
      ],
      "matchUpdateTypes": ["patch", "pin", "digest"],
      "automerge": true
    },
    {
      "matchManagers": ["pre-commit"],
      "matchDatasources": ["github-releases", "github-tags", "pypi"],
      "matchUpdateTypes": ["minor", "patch", "pin", "digest"],
      "automerge": true
    }
  ]
}
