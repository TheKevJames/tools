{
  "automerge": false,
  "branchPrefix": "renovate/",
  "extends": [
    "group:monorepos",
    "group:recommended"
  ],
  "ignoreUnstable": true,
  "lockFileMaintenance": {
    "enabled": true
  },
  "packageRules": [{
    "datasources": ["docker"],
    "packageNames": ["python"],
    "versionScheme": "regex:^(?<major>\\d+)\\.(?<minor>\\d+)\\.(?<patch>\\d+)(?<prerelease>[^.-]+)?(-(?<compatibility>.*))?$"
  }, {
    "datasources": ["orb"],
    "rangeStrategy": "replace",
    "patch": {
      "enabled": false
    }
  }, {
    "packagePatterns": ["^com.google.auto.value"],
    "managers": ["maven"],
    "groupName": "com.google.auto.value packages"
  }, {
    "datasources": ["pypi"],
    "packagePatterns": ["^grpcio"],
    "groupName": "grpcio"
  }, {
    "datasources": ["pypi"],
    "packageNames": ["pytest", "pytest-asyncio", "pytest-cov", "pytest-datadir", "pytest-mock", "hypothesis"],
    "updateTypes": ["minor", "patch", "pin", "digest"],
    "automerge": true,
    "automergeType": "branch"
  }],
  "prConcurrentLimit": 0,
  "prCreation": "immediate",
  "prHourlyLimit": 0,
  "pruneStaleBranches": true,
  "rangeStrategy": "pin",
  "rollbackPrs": true,
  "semanticCommits": true,
  "semanticCommitType": "chore",
  "separateMajorMinor": true,
  "separateMinorPatch": true,
  "separateMultipleMajor": true,
  "unpublishSafe": true,
  "updateNotScheduled": true
}
